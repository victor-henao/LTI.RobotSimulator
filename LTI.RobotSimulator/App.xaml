<Application x:Class="LTI.RobotSimulator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LTI.RobotSimulator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="Open" UriSource="Icons/OpenFile_16x.png" />
        <BitmapImage x:Key="Save" UriSource="Icons/Save_16x.png" />
        <BitmapImage x:Key="Run" UriSource="Icons/Run_16x.png" />
        <BitmapImage x:Key="Pause" UriSource="Icons/Pause_16x.png" />
    </Application.Resources>
</Application>
